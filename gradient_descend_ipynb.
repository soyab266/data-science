        "b=500\n",
        "m=80.2\n",
        "lr=0.01\n",
        "\n",
        "epochs=100\n",
        "for i in range(epochs):\n",
        "  loss_slope= -2* np.sum(y-m*x.ravel()-b)\n",
        "  b=b-(lr*loss_slope)\n",
        "  y_pred= m*x + b\n",
        "\n",
        "  plt.plot(x,y_pred)\n",
        "plt.scatter(x,y)"
